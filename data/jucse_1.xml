<color name="u_base02"      value="#073642"/>
<color name="u_base01"      value="#586e75"/>
<color name="u_base00"      value="#657b83"/>
<color name="u_base0"       value="#839496"/>
<color name="u_base1"       value="#93a1a1"/>
<color name="u_base2"       value="#eee8d5"/>
<color name="u_base3"       value="#fdf6e3"/>
<color name="u_yellow"      value="#b58900"/>
<color name="u_orange"      value="#cb4b16"/>
<color name="u_red"         value="#dc322f"/>
<color name="u_magenta"     value="#d33682"/>
<color name="u_violet"      value="#6c71c4"/>
<color name="u_blue"        value="#268bd2"/>
<color name="u_cyan"        value="#2aa198"/>
